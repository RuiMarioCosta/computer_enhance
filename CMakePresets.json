{
  "version": 3,
  "configurePresets": [
    {
      "name": "conf-common",
      "description": "General settings that apply to all configurations",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "CMAKE_VERBOSE_MAKEFILE": "ON"
      }
    },
    {
      "name": "compiler-gcc",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++"
      }
    },
    {
      "name": "compiler-clang",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_C_COMPILER": "clang",
        "CMAKE_CXX_COMPILER": "clang++"
      }
    },
    {
      "name": "buildtype-debug",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "DEBUG"
      }
    },
    {
      "name": "buildtype-release",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RELEASE"
      }
    },
    {
      "name": "buildtype-coverage",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "COVERAGE"
      },
      "environment": {
        "LC_ALL": "en_US.UTF-8",
        "LC_CTYPE": "en_US.UTF-8"
      }
    },
    {
      "name": "buildtype-asan",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "ASAN"
      }
    },
    {
      "name": "buildtype-msan",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "MSAN"
      }
    },
    {
      "name": "buildtype-tsan",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "TSAN"
      }
    },
    {
      "name": "arch-x86",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_CROSSCOMPILING_EMULATOR": "qemu-x86_64"
      },
      "condition": {
        "lhs": "$env{ARCH}",
        "type": "equals",
        "rhs": "x86"
      }
    },
    {
      "name": "arch-arm64",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_CROSSCOMPILING_EMULATOR": "qemu-aarch64"
      },
      "condition": {
        "lhs": "$env{ARCH}",
        "type": "equals",
        "rhs": "arm64"
      }
    },
    {
      "name": "debug-gcc-x86",
      "inherits": [
        "arch-x86",
        "buildtype-debug",
        "compiler-gcc-x86",
        "conf-common"
      ]
    },
    {
      "name": "debug-clang-x86",
      "inherits": [
        "arch-x86",
        "buildtype-debug",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "release-gcc-x86",
      "inherits": [
        "arch-x86",
        "buildtype-release",
        "compiler-gcc-x86",
        "conf-common"
      ]
    },
    {
      "name": "release-clang-x86",
      "inherits": [
        "arch-x86",
        "buildtype-release",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "coverage-gcc-x86",
      "inherits": [
        "arch-x86",
        "buildtype-coverage",
        "compiler-gcc-x86",
        "conf-common"
      ]
    },
    {
      "name": "coverage-clang-x86",
      "inherits": [
        "arch-x86",
        "buildtype-coverage",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "asan-gcc-x86",
      "inherits": [
        "arch-x86",
        "buildtype-asan",
        "compiler-gcc-x86",
        "conf-common"
      ]
    },
    {
      "name": "asan-clang-x86",
      "inherits": [
        "arch-x86",
        "buildtype-asan",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "msan-clang-x86",
      "inherits": [
        "arch-x86",
        "buildtype-msan",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "tsan-gcc-x86",
      "inherits": [
        "arch-x86",
        "buildtype-tsan",
        "compiler-gcc-x86",
        "conf-common"
      ]
    },
    {
      "name": "tsan-clang-x86",
      "inherits": [
        "arch-x86",
        "buildtype-tsan",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "debug-gcc-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-debug",
        "compiler-gcc-arm64",
        "conf-common"
      ]
    },
    {
      "name": "debug-clang-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-debug",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "release-gcc-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-release",
        "compiler-gcc-arm64",
        "conf-common"
      ]
    },
    {
      "name": "release-clang-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-release",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "coverage-gcc-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-coverage",
        "compiler-gcc-arm64",
        "conf-common"
      ]
    },
    {
      "name": "coverage-clang-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-coverage",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "asan-gcc-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-asan",
        "compiler-gcc-arm64",
        "conf-common"
      ]
    },
    {
      "name": "asan-clang-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-asan",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "msan-gcc-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-msan",
        "compiler-gcc-arm64",
        "conf-common"
      ]
    },
    {
      "name": "msan-clang-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-msan",
        "compiler-clang",
        "conf-common"
      ]
    },
    {
      "name": "tsan-gcc-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-tsan",
        "compiler-gcc-arm64",
        "conf-common"
      ]
    },
    {
      "name": "tsan-clang-arm64",
      "inherits": [
        "arch-arm64",
        "buildtype-tsan",
        "compiler-clang",
        "conf-common"
      ]
    }
  ],
  "buildPresets": [
    {
      "name": "debug-gcc-x86",
      "configurePreset": "debug-gcc-x86"
    },
    {
      "name": "debug-clang-x86",
      "configurePreset": "debug-clang-x86"
    },
    {
      "name": "asan-gcc-x86",
      "configurePreset": "asan-gcc-x86"
    },
    {
      "name": "asan-clang-x86",
      "configurePreset": "asan-clang-x86"
    },
    {
      "name": "coverage-gcc-x86",
      "configurePreset": "coverage-gcc-x86"
    },
    {
      "name": "coverage-clang-x86",
      "configurePreset": "coverage-clang-x86"
    }
  ],
  "testPresets": [
    {
      "name": "coverage-clang-x86",
      "configurePreset": "coverage-clang-x86",
      "output": {
        "outputOnFailure": true
      }
    },
    {
      "name": "coverage-gcc-x86",
      "configurePreset": "coverage-gcc-x86",
      "output": {
        "outputOnFailure": true
      }
    },
    {
      "name": "asan-clang-x86",
      "configurePreset": "asan-clang-x86",
      "output": {
        "outputOnFailure": true
      }
    }
  ]
}
